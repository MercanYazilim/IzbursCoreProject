@model Basvuru
    <script type="text/javascript">
        $(document).ready(function () {
            $("#DurumDanger").hide();
            $("#DurumSuccess").hide();
            $(function () {
                var ID = "";
                $("#btnOgrenciSari").click(function () {
                    ID = $(this).data("id");
                    BursGuncelle(ID);
                });
                $("#btnOgrenciMavi").click(function () {
                    ID = $(this).data("id");
                    BursGuncelle(ID);
                });
                $("#btnOgrenciYesil").click(function () {
                    ID = $(this).data("id");
                    BursGuncelle(ID);
                });
                $("#btnOgrenciKirmizi").click(function () {
                    ID = $(this).data("id");
                    BursGuncelle(ID);
                });
                function BursGuncelle(durumId) {
                    if (durumId != "") {
                        var Id = $("#BasvuruId").val();
                        $.ajax({
                            type: "POST",
                            url: "/Admin/Ogrenci/DurumGuncelle",
                            data: { Id: Id, durumId: durumId },
                            dataType: "json",
                            success: function (data) {
                                if (data == "ok") {
                                    $("#DurumSuccess").show();
                                } else {
                                    $("#DurumDanger").show();
                                }
                            }
                        });
                    }
                }
            });
        });
    </script>
